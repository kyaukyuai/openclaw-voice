{
  "name": "openclaw-voice",
  "version": "1.0.1",
  "main": "index.ts",
  "types": "./dist/package.d.ts",
  "exports": {
    ".": {
      "types": "./dist/package.d.ts",
      "default": "./dist/package.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "build:package": "tsc -p tsconfig.package.json",
    "prepack": "npm run build:package"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@noble/ed25519": "^3.0.0",
    "@noble/hashes": "^2.0.1",
    "base-64": "^1.0.0",
    "expo": "~54.0.33",
    "expo-crypto": "^15.0.8",
    "expo-secure-store": "^15.0.8",
    "expo-speech-recognition": "^3.1.0",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-markdown-display": "^7.0.2"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": false
}
